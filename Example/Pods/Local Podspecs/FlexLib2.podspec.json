{
  "name": "FlexLib2",
  "version": "1.0",
  "summary": "An obj-c flex layout framework for IOS",
  "description": "It's a layout framework based on yoga engine. The main purpose is to provide easy and fast usage.",
  "homepage": "https://github.com/zionfong/FlexLib2.git",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "zionfong@gmail.com": "zionfong@gmail.com"
  },
  "source": {
    "git": "https://github.com/zionfong/FlexLib2.git",
    "tag": "1.0"
  },
  "platforms": {
    "ios": "8.0",
    "osx": "10.10"
  },
  "ios": {
    "source_files": "FlexLib2/Classes/**/*",
    "resource_bundles": {
      "FlexLib2": [
        "FlexLib2/Assets/*"
      ]
    },
    "exclude_files": "FlexLib2/Classes/GDataXMLNode.{h,m}"
  },
  "osx": {
    "source_files": "macosx/Classes/**/*",
    "resource_bundles": {
    },
    "exclude_files": "macosx/Classes/GDataXMLNode.{h,m}"
  },
  "dependencies": {
    "Yoga": [
      "1.14.0"
    ]
  },
  "libraries": "xml2",
  "xcconfig": {
    "HEADER_SEARCH_PATHS": "/usr/include/libxml2"
  },
  "subspecs": [
    {
      "name": "no-arc",
      "ios": {
        "requires_arc": false,
        "source_files": "FlexLib2/Classes/GDataXMLNode.{h,m}"
      }
    },
    {
      "name": "osx-no-arc",
      "osx": {
        "requires_arc": false,
        "source_files": "macosx/Classes/GDataXMLNode.{h,m}"
      }
    }
  ]
}
